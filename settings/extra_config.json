{
    "security/cors": {
        "allow_origins": [
          "*"
        ],
        "allow_methods": [
          "GET",
          "HEAD",
          "POST"
        ],
        "expose_headers": [
          "Content-Length",
          "Content-Type"
        ],
        "allow_headers": [
          "Accept-Language"
        ],
        "max_age": "12h",
        "allow_credentials": false,
        "debug": false
    },
    "router": {
        "return_error_msg": true
    }
}